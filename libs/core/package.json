{
  "name": "@dynamic-msw/core",
  "license": "MIT",
  "sideEffects": false,
  "version": "1.6.0-beta.0",
  "description": "Alter mocked responses on the fly using an infinite amount of configuration parameters. Usefull for testing feature flags, error responses or what ever reason you have to alter your API mocks.",
  "keywords": [
    "dynamic-msw",
    "msw",
    "mock",
    "mocking"
  ],
  "homepage": "https://github.com/dynamicmsw/dynamic-msw/tree/main/libs/core",
  "bugs": {
    "url": "https://github.com/dynamicmsw/dynamic-msw/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dynamicmsw/dynamic-msw.git",
    "directory": "libs/core"
  },
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "readmeFile": "./README.md",
    "displayName": "@dynamic-msw/core",
    "tsconfig": "./tsconfig.lib.json"
  },
  "exports": {
    "./node": {
      "types": "./node/src/index.node.d.ts",
      "require": "./node/index.node.js",
      "default": "./node/index.node.cjs"
    },
    ".": {
      "types": "./src/index.d.ts",
      "require": "./index.js",
      "default": "./index.cjs"
    }
  }
}
